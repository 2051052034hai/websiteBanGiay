@model IEnumerable<WebBanGiayDep.Models.SANPHAM>
@foreach (var item in Model)
{
    <div class="wmuSliderWrapper">
        <article style="position: relative; width: 100%; opacity: 1;">
            <div class="banner-wrap">
                <div class="slider-left">
                    <img src="~/images/@item.AnhBia" alt="" />
                </div>
                <div class="slider-right">
                    @*<h1>@item.MaThuongHieu</h1>*@
                    <h2>@item.GiaBan VND</h2>
                    <p>@item.TenGiay</p>
                    <div class="btn"><a href="#">Xem thêm</a></div>
                </div>
                <div class="clear"></div>
            </div>
        </article>
    </div>
}
