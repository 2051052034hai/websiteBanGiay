@model WebBanGiayDep.Models.THUONGHIEU

@{
    ViewBag.Title = "ChiTietThuongHieu";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<style>
    dt {
        font-size: 16px
    }
</style>
<div class="wrap" style="padding-top:2%; padding-left:10%; padding-right:10%; padding-bottom:5%">
    <h2 style="text-align: center;">THÔNG TIN CHI TIẾT THƯƠNG HIỆU</h2>

    <br />
    <dl class="dl-horizontal">
        <dt>
           Tên thương hiệu:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenThuongHieu)
        </dd>

    </dl>
    <p>
        @Html.ActionLink("Điều chỉnh", "SuaThuongHieu", new { id = Model.MaThuongHieu }) |
        @Html.ActionLink("Trở về trang thương hiệu", "ThuongHieu")
    </p>
</div>

